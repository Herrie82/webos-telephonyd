		<method name="SetProperty">
			<arg type="s" name="property" direction="in"/>
			<arg type="v" name="value" direction="in"/>
		</method>
		<signal name="PropertyChanged">
			<arg type="s" name="property" direction="out"/>
			<arg type="v" name="value" direction="out"/>
